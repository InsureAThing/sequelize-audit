{
  "name": "sequelize-audit-hooks",
  "version": "0.0.17",
  "description": "Push Sequelize auditing messages to a SQS queue",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets env -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "jest"
  },
  "author": "Jack G <jack@insureathing.com>",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  },
  "dependencies": {
    "aws-sdk": "^2.59.0",
    "continuation-local-storage": "^3.2.0",
    "jest": "^20.0.4",
    "no-kafka": "^3.1.4",
    "sqs-producer": "^1.5.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.3.3",
    "eslint": "^3.19.0",
    "jest": "^20.0.4",
    "prettier": "^1.3.1",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-jest": "^19.0.1"
  }
}
